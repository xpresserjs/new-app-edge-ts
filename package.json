{
  "name": "new-app-ts",
  "version": "1.0.1",
  "description": "Xpresser Typescript App Boilerplate using Edge.js Template Engine.",
  "main": "app.ts",
  "repository": "https://github.com/xpresserjs/new-app-ts",
  "author": "xpresserjs",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "npx xjs @stack tsc",
    "ts-watch": "npx tsc --watch",
    "ts-dev": "npx ts-node-dev --respawn app.ts",
    "start": "npm run build && node build/app.js",
    "node-dev": "nodemon build/app.js"
  },
  "dependencies": {
    "express-edge": "^2.0.2",
    "xpresser": "^0.25.4"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.4",
    "xjs-cli": "^1.1.3"
  }
}
